files:
    "/etc/httpd/conf.d/ssl_rewrite.conf":
        mode: "000644"
        owner: root
        group: root
        content: |
            RewriteEngine On
            <If "%{HTTP_HOST} == 'nexxplanner.com'">
            RewriteRule (.*) https://www.%{HTTP_HOST}%{REQUEST_URI} [R,L]
            </If> 
           